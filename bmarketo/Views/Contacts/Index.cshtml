
<section class="contactform">
    ´
    <div class="container">
        <div class="contacthero">
            <img src="Images/Placeholders/1920x300.svg" alt="">
        </div>
        <div class="formsquare">
            <h3>Do you have any questions? contact us here:</h3>
            <hr>

            <form>
                <div class="form-group row">
                    <label>Your Name*</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" placeholder="Enter your name">
                    </div>
                </div>
                <div class="form-group row">
                    <label>Your Email*</label>
                    <div class="col-sm-10">
                        <input type="email" class="form-control" placeholder="Enter your email">
                    </div>
                </div>
                <div class="form-group row">
                    <label>Phone Number</label>
                    <div class="col-sm-10">
                        <input class="form-control" placeholder="Enter your phone number">
                    </div>
                </div>
                <div class="form-group row">
                    <label>Company (optional)</label>
                    <div class="col-sm-10">
                        <input class="form-control" placeholder="Enter your company name ">
                    </div>
                </div>
                <div class="commentsection">
                    <label class="commentlabel">Comment</label>
                    <textarea class="commentarea" rows="3"></textarea>
                </div>
            </form>
            <button class="btn-theme">Submit</button>

        </div>
    </div>

</section>
